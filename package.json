{
  "name": "taro-skeleton",
  "version": "1.0.5",
  "description": "基于Taro的一个骨架屏组件，简单易用",
  "main": "dist/index.js",
  "main:h5": "dist/h5/index.js",
  "keywords": [
    "taro",
    "taro-skeleton",
    "skeleton"
  ],
  "types": "./src/index.tsx",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "build:ui": "TARO_BUILD_TYPE=ui taro build --ui",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "commit": "git-cz",
    "release": "release-it --no-git.requireUpstream",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "author": "lentoo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:lentoo/taro-skeleton.git"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@tarojs/components": "1.3.12",
    "@tarojs/router": "1.3.12",
    "@tarojs/taro": "1.3.12",
    "@tarojs/taro-alipay": "1.3.12",
    "@tarojs/taro-h5": "1.3.12",
    "@tarojs/taro-qq": "1.3.12",
    "@tarojs/taro-quickapp": "1.3.12",
    "@tarojs/taro-swan": "1.3.12",
    "@tarojs/taro-tt": "1.3.12",
    "@tarojs/taro-weapp": "1.3.12",
    "classnames": "^2.2.6",
    "nerv-devtools": "^1.4.0",
    "nervjs": "^1.4.0"
  },
  "devDependencies": {
    "@tarojs/plugin-babel": "1.3.12",
    "@tarojs/plugin-csso": "1.3.12",
    "@tarojs/plugin-sass": "1.3.12",
    "@tarojs/plugin-uglifyjs": "1.3.12",
    "@tarojs/webpack-runner": "1.3.12",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.4.6",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "conventional-changelog-cli": "^2.0.23",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-config-taro": "1.3.12",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-taro": "1.3.12",
    "git-cz": "^3.2.1",
    "release-it": "^12.3.5",
    "stylelint": "9.3.0",
    "stylelint-config-taro-rn": "1.3.12",
    "stylelint-taro-rn": "1.3.12",
    "typescript": "^3.0.1"
  }
}
